						<update>false</update>
						<export>false</export>
						<format>true</format>
						<configurationfile>src/main/resources/hibernate.cfg.xml</configurationfile>
						<propertyfile>src/main/resources/reference-database.properties</propertyfile>
						<outputfilename>createSchema.sql</outputfilename>
					</componentProperties>
				</configuration>
			</plugin>
