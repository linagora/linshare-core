		<cxf.swagger.ui.version>3.25.0</cxf.swagger.ui.version>
		<guava.version>28.2-jre</guava.version>
		<h2.version>1.4.200</h2.version>
		<hibernate.version>5.4.13.Final</hibernate.version>
		<hibernate.validator.version>5.4.3.Final</hibernate.validator.version>
		<jackson.version>2.10.0</jackson.version>
		<jclouds.version>2.2.0</jclouds.version>
		<jjwt.version>0.11.1</jjwt.version>
		<linid.authorization.version>2.1.2</linid.authorization.version>
		<maven.build.timestamp.format>yyyy.MM.dd-HH.mm.ss</maven.build.timestamp.format>
		<mongo.java.server>1.26.0</mongo.java.server>
		<mozilla.rhino.version>1.7.12</mozilla.rhino.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<servlet.context.version>3.0.1</servlet.context.version>
		<slf4j-release-version>1.7.30</slf4j-release-version>
		<spring-data.version>Moore-SR5</spring-data.version>
		<spring.mvc.version>5.1.14.RELEASE</spring.mvc.version>
		<spring-release-version>5.1.14.RELEASE</spring-release-version>
		<spring-security-release-version>5.1.9.RELEASE</spring-security-release-version>
		<sql.driver.postgresql>42.2.12</sql.driver.postgresql>
		<thymeleaf.version>3.0.11.RELEASE</thymeleaf.version>
	</properties>
</project>
