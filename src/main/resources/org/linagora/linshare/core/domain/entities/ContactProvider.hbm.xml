<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.linagora.linshare.core.domain.entities.ContactProvider" table="contact_provider" lazy="false" discriminator-value="3">
		<id name="id" column="id" type="long" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<discriminator column="provider_type" type="string"/>
		<property name="uuid" column="uuid" unique="true" type="string" length="255" not-null="true" lazy="false"/>
		<property name="creationDate" column="creation_date" type="timestamp" not-null="true" lazy="false"/>
		<property name="modificationDate" column="modification_date" type="timestamp" not-null="true" lazy="false"/>
		<subclass name="org.linagora.linshare.core.domain.entities.LdapContactProvider" discriminator-value="LDAP_PROVIDER" lazy="false">
			<property name="baseDn" column="base_dn" type="string" length="255" not-null="false" lazy="false"/>
			<many-to-one name="ldapPattern" cascade="lock" column="ldap_pattern_id" class="org.linagora.linshare.core.domain.entities.ContactLdapPattern" not-null="true" lazy="proxy">
			</many-to-one>
			<many-to-one name="ldapConnexion" cascade="lock" column="ldap_connection_id" class="org.linagora.linshare.core.domain.entities.LdapConnection" not-null="true" lazy="proxy">
			</many-to-one>
		</subclass>
	</class>
</hibernate-mapping>
